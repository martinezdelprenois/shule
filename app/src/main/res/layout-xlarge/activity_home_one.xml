<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_home_one"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.marto.my_school_revierwer_4.com.marto.signup.process.Home_one">


    <Button
        android:text="@string/sign_up"
        android:id="@+id/btn_sign_up"
        android:onClick="click"
        android:layout_marginTop="365dp"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="40sp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="@string/sign_in"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/btn_sign_in"
        tools:ignore="RelativeOverlap"
        android:onClick="click"
        android:layout_marginTop="39dp"
        android:textSize="40sp"
        android:layout_below="@+id/btn_sign_up"
        android:layout_alignLeft="@+id/btn_sign_up"
        android:layout_alignStart="@+id/btn_sign_up" />
</RelativeLayout>
