<?xml version="1.0" encoding="utf-8"?>


    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/ed_email_insti"
        android:layout_alignEnd="@+id/textView6"
        android:layout_alignRight="@+id/textView6"
        tools:ignore="ExtraText">


    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="286dp"
        android:layout_height="114dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="-49dp"></android.support.v7.widget.Toolbar>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignBottom="@+id/toolbar"
        android:layout_centerHorizontal="true"
        android:text="SIGNING UP"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="302dp"
        tools:layout_editor_absoluteY="0dp" />/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/toolbar"
        android:text="Institution"
        tools:ignore="HardcodedText,MissingConstraints"
        android:textSize="30sp"
        tools:layout_editor_absoluteY="103dp"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/ed_institution_insti_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:maxLines="1"
        android:selectAllOnFocus="true"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="546dp"
        tools:layout_editor_absoluteY="200dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Personal Email"
        tools:ignore="HardcodedText,MissingConstraints"
        android:textSize="30sp"
        android:layout_below="@+id/textView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:layout_editor_absoluteY="207dp"
        tools:layout_editor_absoluteX="-7dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Password"
        tools:ignore="HardcodedText,MissingConstraints"
        android:textSize="30sp"
        android:layout_below="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:layout_editor_absoluteY="342dp"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/ed_password_insti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignLeft="@+id/ed_email_insti"
        android:layout_alignStart="@+id/ed_email_insti"
        android:ems="10"
        android:inputType="textPassword"
        android:maxLines="1"
        android:selectAllOnFocus="true"
        tools:ignore="HardcodedText,MissingConstraints,UnknownIdInLayout"
        tools:layout_editor_absoluteY="335dp"
        tools:layout_editor_absoluteX="546dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Institution Website"
        tools:ignore="HardcodedText,MissingConstraints"
        android:textSize="30sp"
        android:layout_above="@+id/textView6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:layout_editor_absoluteY="457dp"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/ed_website_insti"
        android:layout_width="345dp"
        android:layout_height="44dp"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignLeft="@+id/ed_password_insti"
        android:layout_alignStart="@+id/ed_password_insti"
        android:ems="10"
        android:inputType="textWebEmailAddress"
        android:maxLines="1"
        android:selectAllOnFocus="true"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="420dp"
        tools:layout_editor_absoluteY="453dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/index_number_insti"
        android:textSize="30sp"
        android:layout_above="@+id/Spinner_institution"
        tools:layout_editor_absoluteY="613dp"
        tools:layout_editor_absoluteX="0dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/ed_index_insti"
        android:layout_width="347dp"
        android:layout_height="44dp"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:ems="10"
        android:inputType="text|textPersonName"
        android:maxLines="1"
        android:selectAllOnFocus="true"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="418dp"
        tools:layout_editor_absoluteY="605dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Education Level"
        tools:ignore="HardcodedText,MissingConstraints"
        android:layout_above="@+id/textView9"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4"
        android:textSize="30sp"
        tools:layout_editor_absoluteY="715dp"
        tools:layout_editor_absoluteX="-3dp" />

    <Spinner
        android:id="@+id/Spinner_institution"
        android:layout_width="360dp"
        android:layout_height="32dp"
        android:layout_alignLeft="@+id/ed_index_insti"
        android:layout_alignStart="@+id/ed_index_insti"
        android:layout_alignTop="@+id/textView7"
        tools:layout_editor_absoluteY="719dp"
        tools:layout_editor_absoluteX="405dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Category"
        tools:ignore="HardcodedText,MissingConstraints"
        android:textSize="30sp"
        android:layout_above="@+id/btn_signin_institution"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:layout_editor_absoluteY="805dp"
        tools:layout_editor_absoluteX="0dp" />

    <Spinner
        android:id="@+id/spinner_cat"
        android:layout_width="321dp"
        android:layout_height="46dp"
        android:layout_alignLeft="@+id/Spinner_institution"
        android:layout_alignStart="@+id/Spinner_institution"
        android:layout_alignTop="@+id/textView9"
        tools:layout_editor_absoluteY="795dp"
        tools:layout_editor_absoluteX="447dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_signin_institution"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Next"
        tools:ignore="HardcodedText,MissingConstraints"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:textSize="30sp"
        tools:layout_editor_absoluteY="879dp"
        tools:layout_editor_absoluteX="321dp" />

    <EditText
        android:id="@+id/ed_insti"
        android:layout_width="339dp"
        android:layout_height="46dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="433dp"
        tools:layout_editor_absoluteY="93dp"
        tools:ignore="MissingConstraints" />

</android.support.constraint.ConstraintLayout>

